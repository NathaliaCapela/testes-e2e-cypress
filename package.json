  "description": "Sample project to demonstrate e2e tests written with Cypress running on GitHub Actions",
  "main": "index.js",
  "scripts": {
    "cy:open": "cypress open",
    "cy:open:tablet": "cypress open --config viewportWidth=767,viewportHeight=480",
